def is_year_leap(year):
    if year % 4 != 0:
        return 'Год не високосный'
    elif year % 100 != 0:
        return 'Год високосный'
    elif year % 400 != 0:
        return 'Год не високосный'
    else:
        return 'Год високосный'
        
