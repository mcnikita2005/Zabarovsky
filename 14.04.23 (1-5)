Задача 1
import math

def select_numbers_less_than_p(p):
    numbers = []
    n = 2
    while True:
        root_n = math.sqrt(n)
        if root_n >= p:
            break
        numbers.append(root_n)
        n += 1
    return numbers

Задача 2
def print_numbers_between_a_and_b(a, b):
    for i in range(a+1, b):
        print(i)

Задача 3
def print_numbers_between_a_and_b_in_reverse_order(a, b):
    for i in range(b-1, a, -1):
        print(i)

Задача 4
def find_smallest_k(n):
    k = 1
    while 3*k <= n:
        k += 1
    return k, 3*k

Задача 5
def find_largest_k(n):
    k = n // 3
    while 3*k >= n:
        k -= 1
    return k+1
